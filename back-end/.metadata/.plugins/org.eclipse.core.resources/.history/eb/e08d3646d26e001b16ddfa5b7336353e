<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

 <mapper namespace="com.example.demo.mapper.CodMapper">
 	
 	
 	  <select id="getCodAll()" resultType="integer"
      >
         SELECT cod FROM academy
     </select>
     
     <delete id="deleteCod" parameterType="int">
         DELETE FROM academy
         WHERE cod = #{cod}
     </delete>
     
     <select id="fetchCod" resultType="integer" parameterType="int"
      >
         SELECT cod FROM academy where cod=#{cod}
     </select>
     
 </mapper>